load("optmiz");


defstruct(
  bpnn_sta_input(
    bpnn,
    inputs,
    outputs))$

defstruct(
  bpnn_sta_output(
    bpnn,
    total_error))$

bpnn_solve_sta(sta):=block(
  [err_expr,stapts],
  err_expr:bpnn_caculate_symbol_error(sta@bpnn,sta@inputs,sta@outputs),
  stapts:stapoints([err_expr]),
  print(stapts),
  new(
    bpnn_sta_output(
      bpnn:sta@bpnn,
      total_error:0)));