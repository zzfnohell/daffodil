
defstruct(
  bpnn_lms(
    bpnn,
    matrixes,
    inputs,
    outputs))$

bpnn_solve_lms(bpnn,inputs,outputs,max_times):=block(
  [numeric_matrixes,equations,error_expr,derror],
  local(diff_error),
  error_expr:bpnn_caculate_symbol_error(bpnn,inputs,outputs),
  derror:maplist(
    lambda(
      [mat],
      matrixmap(
        lambda(
          [symb],
          diff(error_expr,symbol)),
        mat)),
    bpnn@symbol_matrixes),
  print(derror))$


